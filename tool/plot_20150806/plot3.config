ARCHIVE    :: Arc/sample.tar.gz
OUTDIR     :: Plot3
XPLOT      :: $g$
YPLOT      :: [m_k $g$] / [BETA]
DPLOT      :: <m_k $g$> / [BETA]
PLOTID     :: D L1 BETA
LEGEND     :: "L=%4d" , L1
CONDITION  :: D == 2 && L1 == L2 && J0 == 1.0 && BETA == 2.0
BEFOREPLOT :: set xrange [0:6]
BEFOREPLOT :: set yrange [0.00:]
#BEFOREPLOT :: set logscale y
